{
  "name": "cakephp/cakepackages",
  "description": "self-contained application that automatically tracks cakephp developer's open source code repositories, including applications and plugins",
  "type": "project",
  "keywords" : [],
  "homepage" : "http://plugins.cakephp.org",
  "license" : "MIT",
  "require" : {
    "php" : ">=5.3",
    "composer/installers" : "~1.0",
    "cakephp/cakephp": "2.4.6",
    "cssmin/cssmin": "*",
    "rgrove/jsmin-php": "*",

    "markstory/asset_compress": "v0.12",
    "cakephp/debug_kit" : "2.2.*"
  },
  "config": {
    "preferred-install": "dist",
    "vendor-dir": "Vendor"
  },
  "extra": {
    "heroku": {
      "document-root": "webroot",
      "index-document": "index.php"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "cssmin/cssmin",
        "version": "3.0.1",
        "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
        "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "rgrove/jsmin-php",
        "version": "1.1.2",
        "dist": { "url": "https://raw.github.com/rgrove/jsmin-php/master/jsmin.php", "type": "file" },
        "autoload": { "classmap": [ "jsmin.php" ] }
      }
    }
  ]
}
